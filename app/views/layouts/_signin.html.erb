<nav class="signin">
  <%#binding.pry%>
        <%if current_user || session[:user_id]%>
              <% if current_user%>
                    You are signed in as <%=current_user.email%> |
                    <%=link_to "Sign Out", destroy_user_session_path, :method => "delete"%><br>
                    <%=link_to "Account settings", contact_path(current_user)%>
              <%else%>
                     You are signed in as <%@user.email%>
                     <%=link_to "Account settings", contact_path(current_user)%>
              <% end%>
        <%elsif current_page?('/users/sign_in')%>
        <%else%>
              Sign in <%= link_to "here", new_user_session_path%>
        <%end%><br>
</nav>
